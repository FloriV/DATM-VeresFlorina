<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rezultat"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <TextView
            android:text="Cele nouă tipuri de personalitate conform Eneagramei sunt următoarele:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="29dp"
            android:id="@+id/textView8"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:layout_below="@+id/mytextview"
            android:layout_centerHorizontal="true" />

    <TextView
            android:text="6. Loialul. Tipul responsabil, orientat spre siguranţa personală."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/textView15"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_below="@+id/textView14"
            android:layout_alignLeft="@+id/textView13"
            android:layout_alignStart="@+id/textView13" />

        <TextView
            android:text="5. Cercetătorul. Tipul cerebral, intens."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:id="@+id/textView14"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:layout_below="@+id/textView7"
            android:layout_alignLeft="@+id/textView15"
            android:layout_alignStart="@+id/textView15" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:id="@+id/mytextview"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textStyle="normal|bold"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="1. Reformatorul. Perfecţionistul. Tipul principial, idealist."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="8. Cel căruia îi plac provocările. Tipul puternic, dominator."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="11dp" />

    <TextView
            android:text="2. Altruistul. Tipul grijuliu, care are grijă de ceilalţi."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="3. Cel care reuşeşte. Tipul adaptabil, orientat spre obţinerea succesului."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="4. Individualistul. Romantic, introvertit."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/textView12" />

    <TextView
        android:text="7. Entuziastul. Generalistul. Tipul mereu ocupat, productiv."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_below="@+id/textView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp" />

    <TextView
            android:text="9. Pacifistul. Tipul calm, fără griji, mulţumit de sine."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
        android:id="@+id/textView17"
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:layout_below="@+id/textView16"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="13dp" />
</RelativeLayout>

